<div class="header_divider_top"></div>

<div class="clear">
  <div class="page_header_left col-3">LINEUP</div>
  <div class="page_header_right col-9">Search Results</div>
</div>
<div class="header_divider_bottom"></div>

<div class="artist_white_background">

  <div class="clear">
    <% @artist_search_results.each do |artist| %>
      <div class="artist_module col-6">
        <img class="artist_module__image" src="<%= artist.image %>">
        <div class="artist_module__name"><%= artist.name %></div>
        <div class="artist_module__origin"><%= artist.origin %></div>
      </div>
    <% end %>
  </div>
</div>
