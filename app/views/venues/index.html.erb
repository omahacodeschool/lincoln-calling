 <!-- This page needs more integrating before it's done. -->
<script src="http://maps.google.com/maps/api/js?v=3.18&sensor=false&client=&key=&libraries=geometry&language=&hl=&region="></script> 
<script src="http://google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js"></script>
<script src='http://google-maps-utility-library-v3.googlecode.com/svn/tags/infobox/1.1.9/src/infobox_packed.js' type='text/javascript'></script> 
<script>json_array = <%=raw @hash.to_json %></script>
 <!-- Header that tells user where they are at. -->
<div class="page_header">
  <div class="header_divider_top"></div>
  <div class="page_header__right">Venue Map</div>
  <div class="page_header__left">INFO</div>
  <div class="header_divider_bottom"></div>
</div> 

  

<div class="clear"></div>
<div class="container__map_and_venue map__image">
  <!-- Show Map -->
  <div class="col-8-tablet venue_list no_padding">
    <div class="map">
      <div id="sidebar_builder" style='width: 100%; height: 400px;'></div>
    </div> 
  </div>
  <!-- Venue Profile Sidebar -->
  <div class="col-4-tablet no_padding">
    <div class="venue_profile">
      <% @venues.each do |venue| %>
        <div class="extra_gutter venueInfo <%= venue.id %>">
          <div class="venue_info venueInfo <%= venue.id %>">
            <div class="venue_info__name venueInfo <%= venue.id %>"><%= venue.name %></div>
            <a href="<%= venue.website %>" class="venue_info__link venueInfo <%= venue.id %>"><%= venue.website %></a>
            <div class="venue_info__address venueInfo <%= venue.id %>"><%= venue.address %></div>
          </div>
          <img class="venue_profile__image venueInfo <%= venue.id %>" src="<%= venue.img.url %>"></img>
          <div class="venue_bio venueInfo <%= venue.id %>"><%= venue.info %></div>
        </div>
    <% end %>
    </div>
  </div>
</div>

    <!-- List of Venues -->
  <div class="col-8-tablet no_padding">
          <!-- The text that follows is how the HTML will likely have to look to allow for adding the selected effect to venue locations when selected.  Or rather, the "venues__text--selected" class modifier needs to be added to make that happen via JavaScript. -->
    <div class="venue_list">
      <div id='sidebar_container'></div> 
    </div>
  </div>

  <%= render "sidebars/events" %>  
</div>

  




